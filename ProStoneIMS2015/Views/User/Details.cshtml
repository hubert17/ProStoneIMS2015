    @model ProStoneIMS2015.Models.ApplicationUser

    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
        <h3 class="modal-title">Detail</h3>
    </div>

    <div class="modal-body">
        <div class="table-responsive">
            <table class="table table-bordered table-hover" id="detail-modal">
                <tr>
                    <td width="30%">@Html.LabelFor(m => Model.FirstName):</td>
                    <td width="70%">@Model.FirstName</td>
                </tr>
                <tr>
                    <td>@Html.LabelFor(m => Model.LastName):</td>
                    <td>@Model.LastName</td>
                </tr>
                <tr>
                    <td>@Html.LabelFor(m => Model.Email):</td>
                    <td>@Model.Email</td>
                </tr>
                <tr>
                    <td>@Html.LabelFor(m => Model.Phone):</td>
                    <td>@Model.Phone</td>
                </tr>
            </table>
        </div>


    </div>

    <div class="modal-footer">
        <button class="btn btn-warning" data-dismiss="modal">Close</button>
    </div>
